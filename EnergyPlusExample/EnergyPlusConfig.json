{
  "name": "EnergyPlus_federate_1",
  "core_type": "zmq",
  "log_level": "warning",
  "period": 60,
  "uninterruptible": false,
  "terminate_on_error": true,
  "wait_for_current_time_update": true,
  "publications": [    {
    "global": true,
    "key": "EnergyPlus/whole_building_energy",
    "type": "double",
    "unit": "C"
  }],
  "subscriptions": [{
    "global": true,
    "key": "Controller/Supply Temperature Difference",
    "type": "double",
    "unit": "C"
  },
  {
    "global": true,
    "key": "Controller/Return Temperature Difference",
    "type": "double",
    "unit": "C"
  }]
}
